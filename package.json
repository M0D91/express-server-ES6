{
  "name": "expresstuned",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean transpile",
    "clean": "rimraf transp-server",
    "dev": "cross-env NODE_ENV=development npm-run-all build server",
    "prod": "cross-env NODE_ENV=production npm-run-all build server",
    "server": "node ./transp-server/bin/www",
    "start": "npm run prod",
    "transpile": "babel ./server --out-dir transp-server"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": ["server/*", "public/*"],
    "ignore":[]
  }
}
